<bean class="com.spring.ci.Certi" name="crt">
	<constructor-arg>
		<value>Java IS Good Language</value>
	</constructor-arg>
</bean>


	<bean class="com.spring.ci.Person" name="person">
		<constructor-arg value="kameshwar sah" />
		<constructor-arg value="2000" type="int" />
		<constructor-arg ref="crt"></constructor-arg>
		
	</bean>
	
	
	<bean class="com.spring.ci.Addition" name="add">
		<constructor-arg value="12"  index="1"/>
		<constructor-arg value="42"  index="0"/>
	</bean>

</beans>
